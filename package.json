{
  "name": "tdesign-svelte",
  "title": "tdesign-svelte",
  "version": "0.0.1-alpha.1",
  "description": "TDesign Component for svelte",
  "type": "module",
  "main": "components/index.js",
  "files": [
    "components"
  ],
  "keywords": [
    "svelte",
    "svelte3",
    "svelte kit",
    "tdesign",
    "javascript"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hzx000822/tdesign-svelte.git"
  },
  "scripts": {
    "init": "git submodule init && git submodule update",
    "start": "npm run init && npm run dev",
    "build": "rollup -c",
    "dev": "cd site && vite",
    "site": "cd site && vite build",
    "site:preview": "cd site && vite preview",
    "format": "prettier --write  --plugin prettier-plugin-svelte ."
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.2",
    "less": "^4.2.0",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-toc-done-right": "^4.2.0",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.1.0",
    "rollup": "^2.79.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-multi-input": "^1.4.1",
    "rollup-plugin-styles": "^4.0.0",
    "svelte": "^4.0.5",
    "svelte-routing": "^2.5.0",
    "tdesign-site-components": "^0.13.14",
    "tdesign-theme-generator": "^1.0.15",
    "vite": "^4.4.5",
    "vite-plugin-svelte-md": "^0.1.7"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.5.3"
  }
}
