{
  "align": "<script>\n  import { TRow, TCol } from 'tdesign-svelte';\n</script>\n\n<div style=\"display: flex;flex-direction: column;gap: 16px\">\n  <p>align top</p>\n  <TRow align=\"top\" justify=\"center\">\n    <TCol span=\"3\">\n      <div style=\"height: 100px\">col-3</div>\n    </TCol>\n    <TCol span=\"3\">\n      <div>col-3</div>\n    </TCol>\n    <TCol span=\"3\">\n      <div style=\"height: 100px\">col-3</div>\n    </TCol>\n    <TCol span=\"3\">\n      <div>col-3</div>\n    </TCol>\n  </TRow>\n\n  <p>align middle</p>\n  <TRow align=\"middle\" justify=\"space-around\">\n    <TCol span=\"3\">\n      <div style=\"height: 100px\">col-3</div>\n    </TCol>\n    <TCol span=\"3\">\n      <div>col-3</div>\n    </TCol>\n    <TCol span=\"3\">\n      <div style=\"height: 100px\">col-3</div>\n    </TCol>\n    <TCol span=\"3\">\n      <div>col-3</div>\n    </TCol>\n  </TRow>\n\n  <p>align bottom</p>\n  <TRow align=\"bottom\" justify=\"space-between\">\n    <TCol span=\"3\">\n      <div style=\"height: 100px\">col-3</div>\n    </TCol>\n    <TCol span=\"3\">\n      <div>col-3</div>\n    </TCol>\n    <TCol span=\"3\">\n      <div style=\"height: 100px\">col-3</div>\n    </TCol>\n    <TCol span=\"3\">\n      <div>col-3</div>\n    </TCol>\n  </TRow>\n</div>",
  "base": "<script>\n  import { TRow, TCol } from 'tdesign-svelte';\n\n  const demoCols = [\n    Array(12).fill(1),\n    Array(6).fill(2),\n    Array(4).fill(3),\n    Array(3).fill(4),\n    Array(2).fill(6),\n    Array(1).fill(12),\n  ];\n</script>\n\n{#each demoCols as cols, i}\n  <TRow>\n    {#each cols as col}\n      <TCol span={col}>\n        <div>{col}</div>\n      </TCol>\n    {/each}\n  </TRow>\n{/each}",
  "flex": "<script>\n  import { TRow, TCol } from 'tdesign-svelte';\n</script>\n\n<div style=\"display: flex;flex-direction: column;gap: 16px\">\n  <TRow>\n    <TCol flex=\"2\">\n      <div>2 / 5</div>\n    </TCol>\n    <TCol flex=\"3\">\n      <div>3 / 5</div>\n    </TCol>\n  </TRow>\n\n  <TRow>\n    <TCol flex=\"100px\">\n      <div>100px</div>\n    </TCol>\n    <TCol flex=\"auto\">\n      <div>auto</div>\n    </TCol>\n  </TRow>\n\n  <TRow>\n    <TCol flex=\"1 1 200px\">\n      <div>1 1 200px</div>\n    </TCol>\n    <TCol flex=\"0 1 300px\">\n      <div>0 1 300px</div>\n    </TCol>\n  </TRow>\n\n  <TRow>\n    <TCol flex=\"none\">\n      <div style=\"padding: 0 16px\">none</div>\n    </TCol>\n    <TCol flex=\"auto\">\n      <div>auto with no-wrap</div>\n    </TCol>\n  </TRow>\n</div>",
  "gutter": "<script>\n  import { TRow, TCol } from 'tdesign-svelte';\n\n  const demoCols = Array(8).fill(3);\n  const gutterObj = { xs: 8, sm: 16, md: 24, lg: 32, xl: 32, xxl: 40 };\n</script>\n\n<div style=\"display: flex;flex-direction: column;gap: 16px\">\n  <!-- 只指定水平 gutter，垂直默认为0 -->\n  <span>水平 gutter 为固定值</span>\n  <TRow gutter=\"16\">\n    <TCol span=\"3\">\n      <div>col-3</div>\n    </TCol>\n    <TCol span=\"3\">\n      <div>col-3</div>\n    </TCol>\n    <TCol span=\"3\">\n      <div>col-3</div>\n    </TCol>\n    <TCol span=\"3\">\n      <div>col-3</div>\n    </TCol>\n  </TRow>\n  <!-- 水平 gutter 为响应式 -->\n  <span>水平 gutter 为响应式</span>\n  <TRow gutter={gutterObj}>\n    <TCol span=\"3\">\n      <div>col-3</div>\n    </TCol>\n    <TCol span=\"3\">\n      <div>col-3</div>\n    </TCol>\n    <TCol span=\"3\">\n      <div>col-3</div>\n    </TCol>\n    <TCol span=\"3\">\n      <div>col-3</div>\n    </TCol>\n  </TRow>\n  <!-- 指定水平和垂直 gutter，注：垂直 gutter 只在同一个row组件下面的col组件之间生效 -->\n  <span>水平和垂直 gutter 均为固定值</span>\n  <TRow gutter={[16, 24]}>\n    {#each demoCols as col, i}\n      <TCol span={col}>\n        <div>col-3</div>\n      </TCol>\n    {/each}\n  </TRow>\n  <!-- 水平 gutter 响应式，垂直 gutter 固定值-->\n  <span>水平 gutter 响应式，垂直 gutter 固定值</span>\n  <TRow gutter={[gutterObj, 24]}>\n    {#each demoCols as col, i}\n      <TCol span={col}>\n        <div>col-3</div>\n      </TCol>\n    {/each}\n  </TRow>\n  <!-- 水平和垂直 gutter 均为响应式 -->\n  <span>水平和垂直 gutter 均为响应式</span>\n  <TRow gutter={[gutterObj, gutterObj]}>\n    {#each demoCols as col, i}\n      <TCol span={col}>\n        <div>col-3</div>\n      </TCol>\n    {/each}\n  </TRow>\n</div>",
  "justify": "<script>\n  import { TRow, TCol } from 'tdesign-svelte';\n</script>\n\n<div style=\"display: flex;flex-direction: column;gap: 16px\">\n  <span>align left</span>\n  <TRow justify=\"start\">\n    <TCol span=\"2\">\n      <div>col-2</div>\n    </TCol>\n    <TCol span=\"2\">\n      <div>col-2</div>\n    </TCol>\n    <TCol span=\"2\">\n      <div>col-2</div>\n    </TCol>\n    <TCol span=\"2\">\n      <div>col-2</div>\n    </TCol>\n  </TRow>\n\n  <span>align center</span>\n  <TRow justify=\"center\">\n    <TCol span=\"2\">\n      <div>col-2</div>\n    </TCol>\n    <TCol span=\"2\">\n      <div>col-2</div>\n    </TCol>\n    <TCol span=\"2\">\n      <div>col-2</div>\n    </TCol>\n    <TCol span=\"2\">\n      <div>col-2</div>\n    </TCol>\n  </TRow>\n\n  <span>align right</span>\n  <TRow justify=\"end\">\n    <TCol span=\"2\">\n      <div>col-2</div>\n    </TCol>\n    <TCol span=\"2\">\n      <div>col-2</div>\n    </TCol>\n    <TCol span=\"2\">\n      <div>col-2</div>\n    </TCol>\n    <TCol span=\"2\">\n      <div>col-2</div>\n    </TCol>\n  </TRow>\n\n  <span>space-between</span>\n  <TRow justify=\"space-between\">\n    <TCol span=\"2\">\n      <div>col-2</div>\n    </TCol>\n    <TCol span=\"2\">\n      <div>col-2</div>\n    </TCol>\n    <TCol span=\"2\">\n      <div>col-2</div>\n    </TCol>\n    <TCol span=\"2\">\n      <div>col-2</div>\n    </TCol>\n  </TRow>\n\n  <span>space-around</span>\n  <TRow justify=\"space-around\">\n    <TCol span=\"2\">\n      <div>col-2</div>\n    </TCol>\n    <TCol span=\"2\">\n      <div>col-2</div>\n    </TCol>\n    <TCol span=\"2\">\n      <div>col-2</div>\n    </TCol>\n    <TCol span=\"2\">\n      <div>col-2</div>\n    </TCol>\n  </TRow>\n</div>",
  "offset": "<script>\n  import { TRow, TCol } from 'tdesign-svelte';\n</script>\n\n<div style=\"display: flex;flex-direction: column;gap: 16px\">\n  <TRow>\n    <TCol span=\"4\">\n      <div>col-4</div>\n    </TCol>\n    <TCol span=\"4\" offset=\"4\">\n      <div>col-4 col-offset-4</div>\n    </TCol>\n  </TRow>\n\n  <TRow>\n    <TCol span=\"3\" offset=\"3\">\n      <div>col-3 col-offset-3</div>\n    </TCol>\n    <TCol span=\"3\" offset=\"3\">\n      <div>col-3 col-offset-3</div>\n    </TCol>\n  </TRow>\n\n  <TRow>\n    <TCol span=\"6\" offset=\"3\">\n      <div>col-6 col-offset-3</div>\n    </TCol>\n  </TRow>\n</div>",
  "order": "<script>\n  import { TRow, TCol } from 'tdesign-svelte';\n</script>\n\n<div style=\"display: flex;flex-direction: column;gap: 16px\">\n  <TRow>\n    <TCol span=\"3\" order=\"4\">\n      <div>1 col-3-order-4</div>\n    </TCol>\n    <TCol span=\"3\" order=\"3\">\n      <div>2 col-3-order-3</div>\n    </TCol>\n    <TCol span=\"3\" order=\"2\">\n      <div>3 col-3-order-2</div>\n    </TCol>\n    <TCol span=\"3\" order=\"1\">\n      <div>4 col-3-order-1</div>\n    </TCol>\n  </TRow>\n</div>",
  "responsive": "<script>\n  import { TRow, TCol } from 'tdesign-svelte';\n\n  const xs = { offset: 0, span: 3 };\n  const sm = { offset: 2, span: 3 };\n  const md = { offset: 4, span: 3 };\n  const lg = { offset: 6, span: 3 };\n  const xl = { offset: 8, span: 3 };\n</script>\n\n<div style=\"display: flex;flex-direction: column;gap: 16px\">\n  <span>宽度响应式</span>\n  <TRow>\n    <TCol xs=\"2\" sm=\"4\" md=\"6\" lg=\"8\" xl=\"10\">\n      <div>Col</div>\n    </TCol>\n    <TCol xs=\"10\" sm=\"8\" md=\"6\" lg=\"4\" xl=\"2\">\n      <div>Col</div>\n    </TCol>\n  </TRow>\n\n  <span>其他属性响应式（支持span，offset，order，pull，push）</span>\n  <TRow>\n    <TCol {xs} {sm} {md} {lg} {xl}>\n      <div>Col</div>\n    </TCol>\n  </TRow>\n</div>",
  "sort": "<script>\n  import { TRow, TCol } from 'tdesign-svelte';\n</script>\n\n<div style=\"display: flex;flex-direction: column;gap: 16px\">\n  <TRow>\n    <TCol span=\"9\" push=\"3\">\n      <div>col-9 col-pull-3</div>\n    </TCol>\n    <TCol span=\"3\" pull=\"9\">\n      <div>col-3 col-pull-9</div>\n    </TCol>\n  </TRow>\n</div>"
}
