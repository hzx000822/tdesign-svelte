{
  "base": "<script>\n  import { TTag } from 'tdesign-svelte';\n\n  function handleClick(e) {\n    console.log('click', e);\n  }\n</script>\n\n<div style=\"display: flex;flex-direction: column;gap:16px\">\n  <div class=\"fl\">\n    <a href=\"https://www.tencent.com/zh-cn\" target=\"_blank\">\n      <TTag>超链接</TTag>\n    </a>\n  </div>\n  <div class=\"fl\">\n    <TTag on:click={handleClick}>灰标签</TTag>\n    <TTag theme=\"primary\">标签一</TTag>\n    <TTag theme=\"success\">标签二</TTag>\n    <TTag theme=\"warning\">标签三</TTag>\n    <TTag theme=\"danger\">标签四</TTag>\n  </div>\n  <div class=\"fl\">\n    <TTag variant=\"light\">灰标签</TTag>\n    <TTag theme=\"primary\" variant=\"light\">标签一</TTag>\n    <TTag theme=\"success\" variant=\"light\">标签二</TTag>\n    <TTag theme=\"warning\" variant=\"light\">标签三</TTag>\n    <TTag theme=\"danger\" variant=\"light\">标签四</TTag>\n  </div>\n  <div class=\"fl\">\n    <TTag variant=\"outline\">灰标签</TTag>\n    <TTag theme=\"primary\" variant=\"outline\">标签一</TTag>\n    <TTag theme=\"success\" variant=\"outline\">标签二</TTag>\n    <TTag theme=\"warning\" variant=\"outline\">标签三</TTag>\n    <TTag theme=\"danger\" variant=\"outline\">标签四</TTag>\n  </div>\n  <div class=\"fl\">\n    <TTag variant=\"light-outline\">灰标签</TTag>\n    <TTag theme=\"primary\" variant=\"light-outline\">标签一</TTag>\n    <TTag theme=\"success\" variant=\"light-outline\">标签二</TTag>\n    <TTag theme=\"warning\" variant=\"light-outline\">标签三</TTag>\n    <TTag theme=\"danger\" variant=\"light-outline\">标签四</TTag>\n  </div>\n</div>\n\n<style>\n  .fl {\n    display: inline-flex;\n    gap: 16px;\n  }\n</style>",
  "delete": "<script>\n  import { TTag, TIcon, TInput } from 'tdesign-svelte';\n\n  let inputVisible = false;\n  function handleClose(index) {\n    console.log(index);\n    tags.splice(index, 1);\n    tags = [...tags];\n  }\n  function handleAdd(e) {\n    inputVisible = true;\n  }\n  function handleInputEnter(e) {\n    const val = e.detail;\n    if (val && !tags.some((item) => item.name === val)) {\n      tags.push({ name: val, type: 'default', showClose: true });\n    }\n    tags = [...tags];\n    inputVisible = false;\n  }\n\n  let tags = [\n    {\n      name: '可删除标签可删除标签',\n      type: 'default',\n      showClose: true,\n      maxWidth: 100,\n    },\n    {\n      name: '可删除标签可删除标签',\n      type: 'default',\n      icon: 'discount',\n      showClose: true,\n      maxWidth: 100,\n    },\n    {\n      name: '可删除标签',\n      type: 'default',\n      showClose: true,\n      disabled: true,\n    },\n  ];\n</script>\n\n<div style=\"display: flex;flex-direction: column;gap:16px\">\n  <div class=\"fl\">\n    <TTag theme=\"primary\" closable>标签一</TTag>\n    <TTag theme=\"success\" closable>标签二</TTag>\n    <TTag theme=\"warning\" closable>标签三</TTag>\n    <TTag theme=\"danger\" closable>标签四</TTag>\n  </div>\n  <div class=\"fl\">\n    <TTag theme=\"primary\" variant=\"light\" closable>标签一</TTag>\n    <TTag theme=\"success\" variant=\"light\" closable>标签二</TTag>\n    <TTag theme=\"warning\" variant=\"light\" closable>标签三</TTag>\n    <TTag theme=\"danger\" variant=\"light\" closable>标签四</TTag>\n  </div>\n  <div class=\"fl\">\n    <TTag variant=\"outline\" theme=\"primary\" closable>标签一</TTag>\n    <TTag variant=\"outline\" theme=\"success\" closable>标签二</TTag>\n    <TTag variant=\"outline\" theme=\"warning\" closable>标签三</TTag>\n    <TTag variant=\"outline\" theme=\"danger\" closable>标签四</TTag>\n  </div>\n  <div class=\"fl\">\n    {#each tags as tag, index}\n      <TTag\n        theme={tag.type}\n        closable={tag.showClose}\n        disabled={!!tag.disabled}\n        maxWidth={tag.maxWidth}\n        on:close={() => handleClose(index)}\n      >\n        {tag.name}\n        {#if tag.icon}\n          <TIcon slot=\"icon\" name={tag.icon}></TIcon>\n        {/if}\n      </TTag>\n    {/each}\n  </div>\n  <div class=\"fl\">\n    {#if !inputVisible}\n      <TTag on:click={handleAdd}>\n        <TIcon name=\"add\" />\n        添加标签\n      </TTag>\n    {:else}\n      <TInput size=\"small\" style=\"width: 94px\" on:enter={handleInputEnter} />\n    {/if}\n  </div>\n</div>\n\n<style>\n  .fl {\n    display: inline-flex;\n    gap: 16px;\n  }\n</style>",
  "icon": "<script>\n  import { TTag, TIcon } from 'tdesign-svelte';\n</script>\n\n<div style=\"display: flex;flex-direction: column;gap:16px\">\n  <div>\n    <TTag>\n      <TIcon slot=\"icon\" name=\"discount\" />\n      插槽图标\n    </TTag>\n  </div>\n</div>",
  "longText": "<script>\n  import { TTag } from 'tdesign-svelte';\n</script>\n\n<div style=\"display: flex;flex-direction: column;gap:16px\">\n  <TTag maxWidth=\"150\">默认超八个字超长文本标签超长省略文本标签</TTag>\n</div>",
  "selectable": "<script>\n  import { TCheckTag } from 'tdesign-svelte';\n\n  let checked1 = false;\n  let checked2 = false;\n  let checked3 = false;\n\n  const STYLE_B_UNCHECKED_PROPS = {\n    theme: 'default',\n    variant: 'outline',\n  };\n\n  const STYLE_C_CHECKED_PROPS = {\n    theme: 'primary',\n    variant: 'outline',\n  };\n  \n</script>\n\n<div style=\"display: flex;flex-direction: column;gap:16px\">\n  <div class=\"fl\">\n    <label>StyleA</label>\n    <TCheckTag bind:checked={checked1} style=\"margin-right: 32px\">选中/未选态</TCheckTag>\n    <TCheckTag readonly checked={true}>选中态</TCheckTag>\n    <TCheckTag readonly checked={false}>未选态</TCheckTag>\n    <TCheckTag checked={true} disabled>选中禁用</TCheckTag>\n    <TCheckTag checked={false} disabled>未选禁用</TCheckTag>\n  </div>\n  <div class=\"fl\">\n    <label>StyleB</label>\n    <TCheckTag bind:checked={checked2} uncheckedProps={STYLE_B_UNCHECKED_PROPS} style=\"margin-right: 32px\"\n    >选中/未选态</TCheckTag\n    >\n    <TCheckTag readonly checked={true}>选中态</TCheckTag>\n    <TCheckTag readonly checked={false} uncheckedProps={STYLE_B_UNCHECKED_PROPS}>未选态</TCheckTag>\n    <TCheckTag checked={true} disabled>选中禁用</TCheckTag>\n    <TCheckTag checked={false} disabled uncheckedProps={STYLE_B_UNCHECKED_PROPS}>未选禁用</TCheckTag>\n  </div>\n  <div class=\"fl\">\n    <label>StyleC</label>\n    <TCheckTag\n      bind:checked={checked3}\n      checkedProps={STYLE_C_CHECKED_PROPS}\n      uncheckedProps={STYLE_B_UNCHECKED_PROPS}\n      style=\"margin-right: 32px\">Outline Tag</TCheckTag\n    >\n    <TCheckTag checked={true} readonly checkedProps={STYLE_C_CHECKED_PROPS}>Checked</TCheckTag>\n    <TCheckTag checked={false} readonly uncheckedProps={STYLE_B_UNCHECKED_PROPS}>Unchecked</TCheckTag>\n    <TCheckTag checked={true} disabled checkedProps={STYLE_C_CHECKED_PROPS}>Disabled</TCheckTag>\n    <TCheckTag checked={false} disabled uncheckedProps={STYLE_B_UNCHECKED_PROPS}>Disabled</TCheckTag>\n  </div>\n</div>\n\n<style>\n  .fl {\n    display: inline-flex;\n    gap: 16px;\n  }\n</style>",
  "shape": "<script>\n  import { TTag } from 'tdesign-svelte';\n</script>\n\n<div style=\"display: flex;flex-direction: column;gap:16px\">\n  <div class=\"fl\">\n    <TTag>标签一</TTag>\n    <TTag theme=\"primary\">标签一</TTag>\n  </div>\n  <div class=\"fl\">\n    <TTag shape=\"round\">标签一</TTag>\n    <TTag shape=\"round\" theme=\"primary\">标签一</TTag>\n  </div>\n  <div class=\"fl\">\n    <TTag shape=\"mark\">标签一</TTag>\n    <TTag shape=\"mark\" theme=\"primary\">标签一</TTag>\n  </div>\n</div>\n\n<style>\n  .fl {\n    display: inline-flex;\n    gap: 16px;\n  }\n</style>",
  "size": "<script>\n  import { TTag } from 'tdesign-svelte';\n</script>\n\n<div style=\"display: flex;gap:16px\">\n  <TTag size=\"small\">小型标签</TTag>\n  <TTag>默认标签</TTag>\n  <TTag size=\"large\">大型标签</TTag>\n</div>"
